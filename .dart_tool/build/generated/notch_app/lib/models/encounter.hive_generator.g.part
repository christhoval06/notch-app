class EncounterAdapter extends TypeAdapter<Encounter> {
  @override
  final int typeId = 0;

  @override
  Encounter read(BinaryReader reader) {
    final numOfFields = reader.readByte();
    final fields = <int, dynamic>{
      for (int i = 0; i < numOfFields; i++) reader.readByte(): reader.read(),
    };
    return Encounter(
      id: fields[0] as String,
      date: fields[1] as DateTime,
      partnerName: fields[2] as String,
      orgasmCount: fields[3] as int,
      rating: fields[4] as int,
      notes: fields[5] as String?,
      tags: (fields[6] as List).cast<String>(),
      moodEmoji: fields[7] as String?,
      protected: fields[8] == null ? true : fields[8] as bool,
    );
  }

  @override
  void write(BinaryWriter writer, Encounter obj) {
    writer
      ..writeByte(9)
      ..writeByte(0)
      ..write(obj.id)
      ..writeByte(1)
      ..write(obj.date)
      ..writeByte(2)
      ..write(obj.partnerName)
      ..writeByte(3)
      ..write(obj.orgasmCount)
      ..writeByte(4)
      ..write(obj.rating)
      ..writeByte(5)
      ..write(obj.notes)
      ..writeByte(6)
      ..write(obj.tags)
      ..writeByte(7)
      ..write(obj.moodEmoji)
      ..writeByte(8)
      ..write(obj.protected);
  }

  @override
  int get hashCode => typeId.hashCode;

  @override
  bool operator ==(Object other) =>
      identical(this, other) ||
      other is EncounterAdapter &&
          runtimeType == other.runtimeType &&
          typeId == other.typeId;
}
